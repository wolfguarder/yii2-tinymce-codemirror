{
  "name": "wolfguard/yii2-tinymce-codemirror",
  "description": "Yii2 extension to use TinyMce with Compressor, CodeMirror and FileManager",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "extension",
    "tinymce",
    "wysiwyg",
    "codemirror"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Wolfguard",
      "email": "ivan.fedyaev@gmail.com",
      "homepage": "https://github.com/wolfguarder/yii2-tinymce-codemirror"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "yiisoft/yii2": "~2.0",
    "bower-asset/tinymce-dist": "*",
    "bower-asset/codemirror": "*"
  },
  "autoload": {
    "psr-4": {
      "wolfguard\\tinymce\\": ""
    }
  }
}
